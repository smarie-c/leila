<div class="visible-xs fix-mobile"></div>	
	<aside class="col-sm-2 profil">
		<img src="<%= asset_path( 'anonyme.jpg' ) %>" class="img-responsive img-rounded" alt="Laila El Halfi" />
			<div class="profil-separator"></div>

		<% if locale == :en %>
			<div class="name">
			<%= (t('profil.name') + @index.firstnameen + ' ' + @index.nameen).bbcode_to_html_with_formatting.html_safe %>
			</div>
			<div class="email">
			<a href="https://www.google.com/maps/place/<%= @map %>"  target="_blank">
				<%= (t('profil.addr') + @index.streeten + ' ' + @index.cpen + ' ' + @index.cityen).bbcode_to_html_with_formatting.html_safe %>
			</a> 
			</div>
			<div class="adresse">
			
			</div>
			<iframe
  				frameborder="0" style="border:0"
 				 src="https://www.google.com/maps/embed/v1/place?key=AIzaSyBegs8cCY3HjnUkA7KKxRGKrJkK_YCPYsk&q=<%= @map %>">
			</iframe> 
			<div class="postal">
			<%= (t('profil.phone') + @index.phoneen).bbcode_to_html_with_formatting.html_safe %>
			<%= (t('profil.fax') + @index.faxen).bbcode_to_html_with_formatting.html_safe %>
			<%= (t('profil.mobil') + @index.mobileen).bbcode_to_html_with_formatting.html_safe %>
			</div>
			<% else %>
			<div class="name">
			<%= (t('profil.name') + @index.firstname + ' ' + @index.name).bbcode_to_html_with_formatting.html_safe %>
			</div>
			<div class="email">
			<a href="https://www.google.com/maps/place/<%= @map %>"  target="_blank">
				<%= (t('profil.addr') + @index.street + ' ' + @index.cp + ' ' + @index.city).bbcode_to_html_with_formatting.html_safe %>
			</a>
			</div>
			<div class="adresse">
			
			</div>
			<iframe
  				frameborder="0" style="border:0"
 				 src="https://www.google.com/maps/embed/v1/place?key=AIzaSyBegs8cCY3HjnUkA7KKxRGKrJkK_YCPYsk&q=<%= @map %>">
			</iframe> 
			<div class="postal">
			<%= (t('profil.phone') + @index.phone).bbcode_to_html_with_formatting.html_safe %>
			<%= (t('profil.fax') + @index.fax).bbcode_to_html_with_formatting.html_safe %>
			<%= (t('profil.mobil') + @index.mobile).bbcode_to_html_with_formatting.html_safe %>
			</div>
		<% end %>
			<div class="profil-separator-bottom"></div>
	</aside>
