<!DOCTYPE html>
<html>
<head>
	<meta charset="UTF-8">
	<title><%= t('header.name') %></title>
	<meta name="viewport" content="width=device-width" />
	<%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
	<%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
	<%= csrf_meta_tags %>
</head>
<body class="container">
	<header>
			<div class="col-sm-2 id">
				<span class="nom-laila">Laila El Halfi</span>
				<span class="job-laila">Avocat</span>
			<!-- col-sm-2 ID -->
			</div>
		<!-- col-sm-offset-2 -->
		<div class="first-separator-v"></div>
			<span class="col-sm-1 menu act">Accueil</span>
			<span class="col-sm-1 menu">Services</span>
			<span class="col-sm-2 menu">Nos Compétences</span>
			<span class="col-sm-1 menu">L'Équipe</span>
			<span class="col-sm-1 menu">Contact</span>
		<div class="second-separator-v"></div>
		<div class="col-sm-3 specialiste">
			specialiste en droit du travail
		</div>
	</header>

	<!-- Separe le Header de la Seaction -->
	<div class="contact-header">
		<div class="contact-each">Téléphone :<span class="infos"> 01 45 53 64 16</span></div>
		<div class="contact-each">FAX :<span class="infos"> 01 45 53 74 16</span></div>
		<div class="contact-each">Mobile :<span class="infos"> 06 61 57 45 87</span></div>
		<div class="contact-each">Email :<span class="infos"> lelhalfi@elhalfi-avocats.com</span></div>
	</div>
<section>

<!-- Debut Carousel -->
<div id="carousel-example-generic" class="carousel slide" data-ride="carousel">
  <!-- Indicators -->
  <ol class="carousel-indicators">
    <li data-target="#carousel-example-generic" data-slide-to="0" class="active"></li>
    <li data-target="#carousel-example-generic" data-slide-to="1"></li>
    <li data-target="#carousel-example-generic" data-slide-to="2"></li>
  </ol>

  <!-- Wrapper for slides -->
  <div class="carousel-inner" role="listbox">
    <div class="item">
      <img src="<%= asset_path( 'casablanca.png' ) %>" alt="...">
      <div class="carousel-caption">
        Casablanca
      </div>
    </div>
    <div class="item active">
      <img src="<%= asset_path( 'madison.png' ) %>" alt="...">
      <div class="carousel-caption">
        Madison
      </div>
    </div>
     <div class="item">
     <img src="<%= asset_path( 'jade.png' ) %>" alt="...">
      <div class="carousel-caption">
        Jade
      </div>
    </div>
  </div>

  <!-- Controls -->
  <a class="left carousel-control" href="#carousel-example-generic" role="button" data-slide="prev">
    <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
    <span class="sr-only">Previous</span>
  </a>
  <a class="right carousel-control" href="#carousel-example-generic" role="button" data-slide="next">
    <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
    <span class="sr-only">Next</span>
  </a>
</div>
<!-- Fin Carousel -->

<nav class="col-sm-offset-1">
	<span align="left" class="col-sm-3 nav-article actu act">Actualité</span>
	<div class="article-separator"></div>
	<span align="center" class="col-sm-5 nav-article skills">Nos Compétences</span>
	<div class="article-separator"></div>
	<span align="right" class="col-sm-3 nav-article contact">Contact</span>
</nav>
<article>





</article>


</section>


<footer>
	
</footer>
<!-- Container Principal -->
</body>
</html>

